{
  "name": "dependency-version-checker",
  "version": "0.3.1",
  "description": "This library allows you to run a check for newer versions against your development and production dependencies, both npm and `git://`/`git+ssh://`.",
  "main": "index.js",
  "bin": {
    "check-dep-versions": "cli.js"
  },
  "scripts": {
    "test-case-1": "cd tests/testProj1 && node ../../cli.js && cd ../..",
    "test-case-2": "cd tests/testProj1 && node ../../cli.js lod && cd ../..",
    "test-case-3": "cd tests/testProj1 && node ../../cli.js --show-errors && cd ../..",
    "test": "npm run test-case-1 && npm run test-case-2 && npm run test-case-3"
  },
  "author": "Tadeusz Makuch",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.4",
    "concat-stream": "^2.0.0",
    "semver": "^6.0.0",
    "spawn-shell": "^2.1.0",
    "yargs": "^13.2.4"
  },
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "devDependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tmakuch/dependency-version-checker.git"
  },
  "keywords": [
    "git",
    "npm",
    "check",
    "versions",
    "updates"
  ],
  "bugs": {
    "url": "https://github.com/tmakuch/dependency-version-checker/issues"
  },
  "homepage": "https://github.com/tmakuch/dependency-version-checker#readme"
}
